      "db:start": "docker compose --env-file .env up -d",
      "db:connect": "docker exec -it backenddatabase psql -U user_thusitha -d master-server-database",
      "start": "nodemon --exec ts-node index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "generate": "graphql-codegen",
      "test": "yarn setup:test && jest --detectOpenHandles --forceExit",
